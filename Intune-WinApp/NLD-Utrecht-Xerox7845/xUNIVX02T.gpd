*switch: XrxTrueTypeFontDlOption
{
    *case: TTF_DOWNLOAD_AS_TRUETYPE
    {
        *FontFormat: HPPCL_OUTLINE
        *Command: CmdSetFontID { *Cmd : "" }
		*Command: CmdSelectFontID
		{
			*Cmd: ""
			*Params: LIST(FontHeight, CurrentFontID, TextYRes)
		}

		*Command: CmdSetCharCode
		{
			*Cmd : ""
		}
    }
    *case: TTF_DOWNLOAD_AS_BITMAP
    {
        *FontFormat: HPPCL_RES
        *Command: CmdSetFontID { *Cmd : "" }
		*Command: CmdSelectFontID
		{
			*Cmd: ""
			*Params: LIST(FontHeight, CurrentFontID, TextYRes)
		}

		*Command: CmdSetCharCode
		{
			*Cmd : ""
		}
    }
    *case: TTF_DOWNLOAD_NONE
    {
    }
}

