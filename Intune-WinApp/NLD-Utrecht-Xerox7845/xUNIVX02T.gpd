    *Option: TTF_DOWNLOAD_AS_TRUETYPE
    {
	
	*Name: "Download as TrueType"
    }
    *Option: TTF_DOWNLOAD_AS_BITMAP
    {
	
	*Name: "Download as Bitmap"
    }
